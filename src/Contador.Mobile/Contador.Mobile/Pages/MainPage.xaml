             x:Class="Contador.Mobile.Pages.MainPage"
             xmlns:toolkit="http://xamarin.com/schemas/2020/toolkit">

    <Grid>
        <Label Text="{x:Static theme:IconFonts.Home}" 
               FontFamily="FontAwesome" 
               TextColor="Black" 
               FontSize="30" 
